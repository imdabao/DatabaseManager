<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Nullable>enable</Nullable>
    <PackageProjectUrl>https://github.com/victor-wiki/DatabaseManager</PackageProjectUrl>
    <Description>Convert geography or geometry type between SqlServer, MySql, Oracle and Postgres.</Description>
    <Version>1.0.3</Version>
    <AssemblyVersion>1.0.3</AssemblyVersion>
    <FileVersion>1.0.3</FileVersion>
    <PackageReleaseNotes>upgrade nuget package:"Oracle.ManagedDataAccess.Core"</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SqlServer.Types" Version="160.1000.6" />
    <PackageReference Include="MySqlConnector" Version="2.2.7" />
    <PackageReference Include="Npgsql.NetTopologySuite" Version="7.0.4" />
    <PackageReference Include="Oracle.ManagedDataAccess.Core" Version="23.3.1-dev" />
  </ItemGroup>

</Project>
